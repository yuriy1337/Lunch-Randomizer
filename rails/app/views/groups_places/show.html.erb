<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", groups_places_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_groups_place_path %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.show", :default => "Show")}", groups_place_path %></li>
    </ul>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b><%= t("activerecord.attributes.groups_place.group_id", :default => t("activerecord.labels.group_id", :default => "Group")) %>:</b>
          <%= @groups_place.group_id %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.groups_place.place_id", :default => t("activerecord.labels.place_id", :default => "Place")) %>:</b>
          <%= @groups_place.place_id %>
        </p>
      <div class="wat-cf">
        <%= link_to image_tag("web-app-theme/icons/application_edit.png", :alt => "#{t("web-app-theme.edit", :default=> "Edit")}") + " " + t("web-app-theme.edit", :default=> "Edit"), edit_groups_place_path(@groups_place), :class => "button" %> 
        <%= link_to image_tag("web-app-theme/icons/cross.png", :alt => "#{t("web-app-theme.delete", :default=> "Delete")}") + " " + t("web-app-theme.delete", :default => "Delete"), groups_place_path(@groups_place), :method => "delete", :class => "button", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
      </div>      
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>